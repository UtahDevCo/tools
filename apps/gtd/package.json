    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-db": "^0.1.30",
    "@tanstack/react-form": "^0.29.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "wrangler": "^4.43.0"
  },
  "name": "bun-react-template",
  "private": true,
  "scripts": {
    "build": "bun run build.ts",
    "build:worker": "tsc --project tsconfig.worker.json",
    "dev": "PORT=3020 bun --hot src/index.tsx",
    "dev:worker": "wrangler dev --port 3020",
    "dev:worker:remote": "wrangler dev --remote",
    "start": "NODE_ENV=production bun src/index.tsx",
    "deploy": "wrangler deploy --env dev",
    "deploy:prod": "wrangler deploy --env production",
    "routes:generate": "npx @tanstack/router-cli generate",
    "routes:watch": "npx @tanstack/router-cli watch"
  },
  "type": "module",
  "version": "0.1.0"
}
